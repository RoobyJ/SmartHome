{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Yarp": "Debug",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.AspNetCore.HttpLogging": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ReplaceHostWithUri": "https://localhost:3020",
  "OpenIDConnect": {
    "Authority": "https://localhost:3022",
    "Scopes": [
      "openid",
      "profile",
      "email",
      "offline_access"
    ]
  },
  "ReverseProxy": {
    "Clusters": {
      "api": {
        "Destinations": {
          "api": {
            "Address": "https://localhost:3022/"
          }
        }
      }
    }
  }
}
